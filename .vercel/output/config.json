{
  "version": 3,
  "routes": [
    {
      "src": "^/_astro/(.*)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "^/(.*)\\.(js|css|svg|ico|jpg|jpeg|png|gif|webp|woff|woff2|ttf|otf)$",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.html",
      "status": 200
    }
  ]
}
